<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="loginapp.TeacherRegistrationController">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <TextField fx:id="numeField" promptText="Nume"/>
    <TextField fx:id="prenumeField" promptText="Prenume"/>
    <TextField fx:id="usernameField" promptText="Username"/>
    <PasswordField fx:id="passwordField" promptText="Parola"/>
    <Button text="Înregistrează" onAction="#handleSubmitButton"/>
</VBox>
